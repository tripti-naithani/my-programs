def first_10_pairs():
    pairs = []
    count = 0
    while(count < 10):
        for i in range(1000):
            for j in range(1000):
                for k in range(1000):
                    if(pow(i,3) + pow(j, 3) == pow(k,2)):
                        pairs.append((i, j, k))
                        count = count + 1
    return pairs

print(first_10_pairs())                                   
            

                    
